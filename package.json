{
  "name": "ojlinks-api",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "nodemon ./bin/www",
    "local": "export ENV=staging && npx serverless offline start",
    "offline": "serverless offline",
    "deploy": "serverless deploy --verbose",
    "deploy:changes": "npm run deploy:func",
    "deploy:func": "serverless deploy function --function api",
    "deploy:live": "serverless deploy --stage production --region us-west-2",
    "upload:fast": "serverless deploy --aws-s3-accelerate"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "express": "~4.16.1",
    "express-validator": "^6.14.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "mysql2": "^2.2.2",
    "passport": "^0.6.0",
    "passport-unique-token": "^3.0.0",
    "pug": "2.0.0-beta11",
    "sequelize": "^6.3.5",
    "serverless-http": "^3.0.2",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "sequelize-cli": "^6.2.0",
    "serverless-offline": "^9.2.2"
  }
}
